    });
    </script>
    <script>
    $(document).ready(function() {
		
		//WOW Scroll Spy
		var wow = new WOW({
			//disabled for mobile
			mobile: false,
			offset:       100
		});
		wow.init();
				
		$('html,body').animate({
		  scrollTop: 0
		}, 1000);
	
		
		$(".manufacturer_btn").click(function(){
			$("#content_holder").hide();
			$("#form_holder").show();
			$("#manufacturer").show();
			$("#trader").hide();
			$("#importer").hide();
			$("#seller_type").val('manufacturer');
				
		});
		$(".trader_btn").click(function(){
			$("#content_holder").hide();
			$("#form_holder").show();
			$("#manufacturer").hide();	
			$("#trader").show();
			$("#importer").hide();
			$("#seller_type").val('trader');
		});
		$(".importer_btn").click(function(){
			$("#content_holder").hide();
			$("#form_holder").show();
			$("#manufacturer").hide();
			$("#trader").hide();
			$("#importer").show();	
			$("#seller_type").val('important');
		});
		
	});
	
	
	
	</script>
